<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>Feed settings</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <switchableControllerActions>
            <TCEforms>
              <label>Select function</label>
              <config>
                <type>select</type>
                <items>
                  <numIndex index="0">
                    <numIndex index="0">List</numIndex>
                    <numIndex index="1">Feed->list</numIndex>
                  </numIndex>
                  <numIndex index="1">
                    <numIndex index="0">Details</numIndex>
                    <numIndex index="1">Feed->detail</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </switchableControllerActions>
          <settings.flexform.listPid>
            <TCEforms>
              <label>Page with list view plugin (List PID)</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>pages</allowed>
                <size>1</size>
                <maxitems>1</maxitems>
                <minitems>0</minitems>
                <show_thumbs>1</show_thumbs>
                <wizards>
                  <suggest>
                    <type>suggest</type>
                  </suggest>
                </wizards>
              </config>
            </TCEforms>
          </settings.flexform.listPid>
          <settings.flexform.singlePid>
            <TCEforms>
              <label>Page with single view plugin (Single PID)</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>pages</allowed>
                <size>1</size>
                <maxitems>1</maxitems>
                <minitems>0</minitems>
                <show_thumbs>1</show_thumbs>
                <wizards>
                  <suggest>
                    <type>suggest</type>
                  </suggest>
                </wizards>
              </config>
            </TCEforms>
          </settings.flexform.singlePid>
          <settings.flexform.feed>
            <TCEforms>
              <label>Feed für Stützpunktliste</label>
              <config>
                <type>select</type>
                <items type="array">
                </items>
                <maxitems>100</maxitems>
                <size>4</size>
                <autoSizeMax>10</autoSizeMax>
                <minitems>0</minitems>
                <foreign_table>tx_iffluidfeed_domain_model_feed</foreign_table>
                <foreign_table_where>AND tx_iffluidfeed_domain_model_feed.deleted = 0 AND tx_iffluidfeed_domain_model_feed.hidden = 0 AND tx_iffluidfeed_domain_model_feed.sys_language_uid = 0 order by tx_iffluidfeed_domain_model_feed.url</foreign_table_where>
                <requestUpdate>1</requestUpdate>
                <wizards>
                  <_PADDING>16</_PADDING>
                <_VERTICAL></_VERTICAL>
              <add>
                <type>script</type>
                <title>Add Form</title>
                <icon>add.gif</icon>
                <params>
                  <table>tx_iffluidfeed_domain_model_feed</table>
                  <pid>###CURRENT_PID###</pid>
                  <setValue>set</setValue>
                </params>
                <script>wizard_add.php</script>
              </add>
              <suggest>
                <title>Add Feed</title>
                <type>suggest</type>
              </suggest>
            </wizards>
          </config>
        </TCEforms>
      </settings.flexform.feed>
  <settings.flexform.customerhousefeed>
    <TCEforms>
      <label>Feed für Kundenhäuser</label>
      <config>
        <type>select</type>
        <items type="array">
        </items>
        <maxitems>100</maxitems>
        <size>4</size>
        <autoSizeMax>10</autoSizeMax>
        <minitems>0</minitems>
        <foreign_table>tx_iffluidfeed_domain_model_feed</foreign_table>
        <foreign_table_where>AND tx_iffluidfeed_domain_model_feed.deleted = 0 AND tx_iffluidfeed_domain_model_feed.hidden = 0 AND tx_iffluidfeed_domain_model_feed.sys_language_uid = 0 order by tx_iffluidfeed_domain_model_feed.url</foreign_table_where>
        <requestUpdate>1</requestUpdate>
        <wizards>
          <_PADDING>16</_PADDING>
        <_VERTICAL></_VERTICAL>
      <add>
        <type>script</type>
        <title>Add Form</title>
        <icon>add.gif</icon>
        <params>
          <table>tx_iffluidfeed_domain_model_feed</table>
          <pid>###CURRENT_PID###</pid>
          <setValue>set</setValue>
        </params>
        <script>wizard_add.php</script>
      </add>
      <suggest>
        <title>Add Feed</title>
        <type>suggest</type>
      </suggest>
    </wizards>
  </config>
</TCEforms>
  </settings.flexform.customerhousefeed>
  <settings.flexform.events>
    <TCEforms>
      <label>Feed für Events</label>
      <config>
        <type>select</type>
        <items type="array">
        </items>
        <maxitems>100</maxitems>
        <size>4</size>
        <autoSizeMax>10</autoSizeMax>
        <minitems>0</minitems>
        <foreign_table>tx_iffluidfeed_domain_model_feed</foreign_table>
        <foreign_table_where>AND tx_iffluidfeed_domain_model_feed.deleted = 0 AND tx_iffluidfeed_domain_model_feed.hidden = 0 AND tx_iffluidfeed_domain_model_feed.sys_language_uid = 0 order by tx_iffluidfeed_domain_model_feed.url</foreign_table_where>
        <requestUpdate>1</requestUpdate>
        <wizards>
          <_PADDING>16</_PADDING>
        <_VERTICAL></_VERTICAL>
      <add>
        <type>script</type>
        <title>Add Form</title>
        <icon>add.gif</icon>
        <params>
          <table>tx_iffluidfeed_domain_model_feed</table>
          <pid>###CURRENT_PID###</pid>
          <setValue>set</setValue>
        </params>
        <script>wizard_add.php</script>
      </add>
      <suggest>
        <title>Add Feed</title>
        <type>suggest</type>
      </suggest>
    </wizards>
  </config>
</TCEforms>
  </settings.flexform.events>
  <settings.flexform.pagination>
    <TCEforms>
      <label>Enable pagination?</label>
      <config>
        <type>check</type>
      </config>
    </TCEforms>
  </settings.flexform.pagination>
  <settings.flexform.perpage>
    <TCEforms>
      <label>Items per page (defaults to 5)</label>
      <config>
        <type>input</type>
        <size>30</size>
      </config>
    </TCEforms>
  </settings.flexform.perpage>
  <settings.flexform.limit>
    <TCEforms>
      <label>Max Items to Show</label>
      <config>
        <type>input</type>
        <size>5</size>
      </config>
    </TCEforms>
  </settings.flexform.limit>
  <settings.templateLayout>
    <TCEforms>
    <label>Layout</label>
      <config>
        <type>select</type>
        <itemsProcFunc>\Interfrog\IfFluidfeed\Hooks\FluidfeedFlexForm->user_templateLayout</itemsProcFunc>
        <items>
          <numIndex index="0" type="array">
            <numIndex index="0"></numIndex>
            <numIndex index="1"></numIndex>
          </numIndex>
        </items>
      </config>
    </TCEforms>
  </settings.templateLayout>
</el>
  </ROOT>
</sDEF>
 </sheets>
</T3DataStructure>
